{
  "name": "clj-monaco",
  "version": "0.0.1",
  "description": "A ClojureScript library for Monaco Editor",
  "main": "",
  "scripts": {
    "shadow:watch": "npx shadow-cljs watch app test",
    "shadow:build": "npx shadow-cljs release app",
    "shadow:server": "npx shadow-cljs server",
    "monaco:watch": "npx rollup -c -w",
    "monaco:build": "npx rollup -c",
    "test:build": "npx shadow-cljs compile ci",
    "test:watch": "npx shadow-cljs watch test",
    "dev": "npx run-p monaco:watch shadow:watch",
    "build": "npx run-s monaco:build shadow:build",
    "serve": "npx http-server ./public -p 3000"
  },
  "engines": {
    "node": ">=13.6.0",
    "npm": ">=6.13.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/just-sultanov/clj-monaco.git"
  },
  "keywords": [
    "clojure",
    "clojurescript",
    "monaco",
    "monaco-editor"
  ],
  "author": "Ilshat Sultanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/just-sultanov/clj-monaco/issues"
  },
  "homepage": "https://github.com/just-sultanov/clj-monaco",
  "dependencies": {
    "create-react-class": "^15.6.3",
    "monaco-editor": "^0.19.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-url": "^4.0.1",
    "autoprefixer": "^9.7.3",
    "highlight.js": "9.17.1",
    "http-server": "^0.12.1",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "react-highlight.js": "1.0.7",
    "rollup": "^1.29.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "shadow-cljs": "^2.8.83",
    "tailwindcss": "^1.1.4",
    "typescript": "^3.7.4"
  }
}
